{
  "name": "devp2p",
  "version": "0.2",
  
  "sockets": {
    "udp": {
      "send": ["*"],
      "bind": ["*"]
    },
    "tcp" : {
      "connect": ["*"]
    },
    "tcpServer" : {
      "listen": ["*"]
    }
  },
  "permissions": [
  	"background",
  	"notifications",
  	"experimental",
  	{"socket": [
        "udp-send-to"
    ]}
  ],
  "app": {
	"background": {
		"scripts": ["app.js"]
    }
  },
  "manifest_version": 2
}
